package nl.numblock.flowcharter {	import nl.numblock.flowcharter.controller.ApplicationPrepCommand;
	import nl.numblock.flowcharter.constants.Notifications;	import org.puremvc.as3.multicore.patterns.facade.Facade;	public class ApplicationFacade extends Facade 	{		public function ApplicationFacade( key : String, _app : Main) 		{			super(key);			sendNotification(Notifications.CREATE, _app);			} 		public static function getInstance( key : String, _app : Main  ) : ApplicationFacade 		{			if ( instanceMap[ key ] == null ) instanceMap[ key ] = new ApplicationFacade(key, _app);			return instanceMap[ key ] as ApplicationFacade;		}		override protected function initializeController() : void 		{			super.initializeController();			registerCommand(Notifications.CREATE, ApplicationPrepCommand);			}		public function startup() : void 		{			sendNotification(Notifications.STARTUP);		}			}}